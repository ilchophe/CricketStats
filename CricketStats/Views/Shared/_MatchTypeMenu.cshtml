@model IEnumerable<CricketStats.Models.MatchType>

<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Matches <span class="caret"></span></a>
    <ul class="dropdown-menu">
        @foreach (var item in Model)
        {
            <li>@Html.ActionLink(item.matchtypename, "Index/" + item.matchtypeid, "Matches")</li>
        }
    </ul>
</li>



